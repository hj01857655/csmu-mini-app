# CSMU教务系统 - 开发环境配置

# ===== 基础配置 =====
NODE_ENV=development
VUE_APP_ENV=development

# ===== API配置 =====
VUE_APP_API_BASE_URL=http://localhost:8000/api/v1
VUE_APP_API_TIMEOUT=10000
VUE_APP_API_RETRY_TIMES=3

# ===== 功能开关 =====
VUE_APP_ENABLE_MOCK=true
VUE_APP_DEBUG=true
VUE_APP_ENABLE_LOG_UPLOAD=false

# ===== 安全配置 =====
VUE_APP_FORCE_HTTPS=false
VUE_APP_TOKEN_EXPIRE_MINUTES=30
VUE_APP_TOKEN_REFRESH_BEFORE_MINUTES=5

# ===== 开发配置 =====
VUE_APP_DEV_PORT=8080
VUE_APP_DEV_HOST=localhost
VUE_APP_HOT_RELOAD=true

# ===== 调试配置 =====
VUE_APP_CONSOLE_LOG=true
VUE_APP_NETWORK_LOG=true
VUE_APP_ERROR_LOG=true
